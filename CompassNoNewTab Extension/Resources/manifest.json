{
  "manifest_version": 3,
  "name": "Remove Target Blank for All Compass Sites",
  "version": "1.0",
  "description": "Removes target='_blank' from links on all Compass Education sites",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.compass.education/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
